
@{
    ViewBag.Title = "Index";
}


<div style="display: none;">
    <h3>@ViewBag.hello</h3>
    <h3>@ViewBag.taxonomyName</h3>
    <h4>Count:@ViewBag.items.Count</h4>
    @foreach (var titlePart in ViewBag.items)
    {
        <h2>@titlePart.Title</h2>
        <h4>@titlePart.Text</h4>
    }
</div>

<div id="carousel-home" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators carousel-indicators-numbers">
        <li data-target="#carousel-home" data-slide-to="0" class="active">1</li>
        <li data-target="#carousel-home" data-slide-to="1">2</li>
        <li data-target="#carousel-home" data-slide-to="2">3</li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
        <div class="item active" style="background-image: url(@Url.Content(Html.ThemePath(WorkContext.CurrentTheme, "/Content/images/top-bg.png")));">
            
            <div class="carousel-caption">
                <div class="row">
                    
                    <div class="col-md-5 col-md-offset-1">关于2017年春季学期“SAF交流生项目”宣讲会通知</div>
                </div>
            </div>
        </div>
        <div class="item" style="background-image: url(@Url.Content(Html.ThemePath(WorkContext.CurrentTheme, "/Content/images/top-bg.png")));">
           
            <div class="carousel-caption">
                <div class="row">
                    <div class="col-md-2"></div>
                    <div class="col-md-5">关于2017年春季学期“SAF交流生项目”宣讲会通知</div>
                </div>
            </div>
        </div>
        <div class="item" style="background-image: url(@Url.Content(Html.ThemePath(WorkContext.CurrentTheme, "/Content/images/top-bg.png")));">
            
            <div class="carousel-caption">
                <div class="row">
                    <div class="col-md-2"></div>
                    <div class="col-md-5">关于2017年春季学期“SAF交流生项目”宣讲会通知</div>
                </div>
            </div>
        </div>
    </div>
</div>

