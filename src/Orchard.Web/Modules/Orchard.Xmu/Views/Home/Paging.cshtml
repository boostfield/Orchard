 @{ 

     var total = ViewBag.total;
     var items = ViewBag.items;
     var page = ViewBag.page;
     var pagesize = ViewBag.pageSize;
 }

<div class="container">
    <ul class="list-group">
        @foreach (var item in items)
        {
            <li class="list-group-item">
                <div class="row">
                    <div class="col-sm-9 title-name">@item.Title</div>
                    <div class="col-sm-3 title-time">@item.PublishedUtc</div>
                </div>

            </li>
        }
    </ul>

    <div class="row">
        <div id="pagination"></div>
    </div>
</div>
<script>
    $('#pagination').bootpag({
        total: parseInt((@total - 1) / 10) ,
        page:  @page,
        maxVisible: 10
        }).on('page', function (event, num) {
            window.location = (location.protocol + '//' + location.host + location.pathname + "?page=" + num + "&pageSize=10");
    });
</script>